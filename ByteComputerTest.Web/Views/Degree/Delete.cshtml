@model ByteComputerTest.Core.Degree
@{
}

<h2>Delete Degree @Model?.Name       </h2>
<hr />

@if (ViewData["ErrorMessage"] != null)
{
    <p class="text-danger">@ViewData["ErrorMessage"]</p>
}
<div class="col-md-12">



    <label asp-for="Id" class="control-label"></label>
    <div class="mb-3">
        @Model?.Id
    </div>


    <label asp-for="Name" class="control-label"></label>
    @Model?.Name
    <div class="mb-3">

        <label asp-for=" Candidate" class="control-label"></label>
        @Model?.Candidate
    </div>
    <div class="mb-3">
        <label asp-for="CreationTime" class="control-label"></label>
        @Model?.CreationTime
    </div>
    <div class="mb-3">
        <label asp-for="CandidateId" class="control-label"></label>
        @Model?.CandidateId
    </div>
    
    <label asp-for="CreationTime" class="control-label"></label>
    <div class="mb-3">
        @Model?.CreationTime
    </div>


</div>


<form method="post" asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Delete Degree" class="btn btn-danger" />
</form>

<div>
    <a asp-controller="Degree" asp-action="Index">Back to overview</a>
</div>